{
  "name": "swagger-express-middleware",
  "version": "1.0.0-alpha.12",
  "description": "Swagger middleware and mocks for Express",
  "keywords": [
    "express",
    "swagger",
    "middleware",
    "mock",
    "fake",
    "stub",
    "rest",
    "api",
    "json"
  ],
  "author": {
    "name": "James Messinger",
    "url": "http://bigstickcarpet.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/BigstickCarpet/swagger-express-middleware",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint .",
    "build": "npm run lint",
    "mocha": "mocha --bail --recursive tests/fixtures tests/specs",
    "istanbul": "c8 --reporter=html --reporter=text --reports-dir=coverage/node mocha --recursive tests/fixtures tests/specs",
    "test": "npm run istanbul",
    "upgrade": "ncu \"/(!multer)/\" --upgradeAll && npm update",
    "bump": "bump --prerelease --tag --push --all",
    "release": "npm run upgrade && npm run build && npm test && npm run bump && npm publish",
    "start": "cd samples && node sample2.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BigstickCarpet/swagger-express-middleware.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "basic-auth": "^1.0.3",
    "c8": "^10.1.2",
    "chai": "^4.5.0",
    "chai-datetime": "^1.8.1",
    "eslint": "^9.21.0",
    "express": "^4.16.3",
    "mocha": "^10.8.2",
    "npm-check-updates": "^16.14.20",
    "sinon": "^1.16.1",
    "supertest": "^7.1.4",
    "version-bump-prompt": "^1.5.1"
  },
  "dependencies": {
    "ajv": "^6.5.4",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.3.5",
    "debug": "^2.2.0",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1",
    "multer": "^2.0.2",
    "ono": "^1.0.22",
    "swagger-methods": "^1.0.0",
    "swagger-parser": "^10.0.3",
    "tmp": "^0.2.5",
    "type-is": "^1.6.8"
  },
  "engines": {
    "node": ">=0.10.36",
    "npm": ">=2.0"
  },
  "peerDependencies": {
    "express": "4.x"
  }
}
